<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MenuItemView">

        <attr name="position" format="enum">
            <enum name="first" value="0"/>
            <enum name="middle" value="1"/>
            <enum name="last" value="2"/>
        </attr>
        <attr name="status" format="enum">
            <enum name="locked" value="0"/>
            <enum name="empty" value="1"/>
            <enum name="incomplete" value="2"/>
            <enum name="complete" value="3"/>
        </attr>

    </declare-styleable>
</resources>